{
  "name": "react-filterable-content",
  "version": "1.0.0-beta.1",
  "description": "React library to make any react component is searchable and filterable",
  "author": "Adnan Hidayat P <komputok@gmail.com>",
  "license": "MIT",
  "repository": "xenovon/react-filterable-content",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "react",
    "filter",
    "filterable",
    "search",
    "list",
    "searchable"
  ],
  "scripts": {
